{
  "orchestrator": { // rename to Orchestration
    "DynamicManagementConfig": {
      "Url": "http:/localhost:7666/node/management",
      "ReadInterval": "1m",
      "ResetTimeout": "30m"
    },
    "storage-driver": "nfs", // rename to StorageDriver
    "storage-options": { // rename to StorageOptions
      "maxRetries": "10"
    }
  },
  "chains": [ // rename to VirtualChains
    {
      "Id": 42,
      "InternalHttpPort": 8080, // for public api, identical for all vchains
      "InternalPort": 4400, // for gossip, identical for all vchains
      "ExternalPort": 4001, // for gossip, different for all vchains
      "InternalHttpPort": 8080, // identical for all vchains
      "Disabled": false,
      "DockerConfig": {
        "Image":  "orbsnetwork/node",
        "Tag":    "v1.1.0",
        "Pull":   true,
        "Resources": {
          "Limits": {
            "Memory": 1024,
            "CPUs": 1
          },
          "Reservations": {
            "Memory": 512,
            "CPUs": 0.5
          }
        }
      },
      "Config": {
        "ManagementConfigUrl": "http://1.1.1.1/vchains/42/management",
        "SignerUrl": "http://1.1.1.1/signer"           
      }
    }
  ],
  "services": { // rename to NodeServices
    "signer": {
      "InternalPort": 7777,
      "DockerConfig": {
        "Image":  "orbsnetwork/orbs-network-signer",
        "Tag":    "v1.1.0",
        "Pull":   true
      },
      "Config": {
        "api": "v1"
      }
    },
    "management-service": {
      "InternalPort": 8080,
      "ExternalPort": 7666,
      "DockerConfig": {
        "Image":  "orbsnetwork/management-service",
        "Tag":    "v1.1.0",
        "Pull":   true,
      },
      "Config": {
        "EthereumGenesisContract": "0x2384723487623784638272348",
        "EthereumEndpoint": "http://eth.orbs.com"
      }
    }
  }
}
